{
	"name": "vue-text-editor-fy",
	"version": "0.1.1",
	"private": false,
	"description": "A vue text editor to beautify the text and text box with elegant editing properties.",
	"author": {
		"name": "Neha Soni",
		"email": "nehaswrankar988@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Nehasoni988/vue-text-editor-fy"
	},
	"bugs": "https://github.com/Nehasoni988/vue-text-editor-fy/issues",
	"keywords": [
		"vue2",
		"vuetify2",
		"text",
		"editor"
	],
	"license": "MIT",
	"files": [
		"dist/*",
		"src/index.d.ts"
	],
	"main": "./dist/vue-text-editor-fy.common.js",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"build-bundle": "vue-cli-service build --target lib --name vue-text-editor-fy ./src/index.js",
		"publish-package": "npm run build && npm run build-bundle && npm version patch -m \"build: release %s\" && npm publish"
	},
	"dependencies": {
		"core-js": "^3.6.5",
		"vue": "^2.6.11",
		"vuetify": "^2.4.11"
	},
	"devDependencies": {
		"@mdi/font": "^5.9.55",
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"vue-template-compiler": "^2.6.11"
	}
}
